    "\n",
    "from pylab import *\n",
    "val = [nb,knn,svm,lr,mlp]   # the bar lengths\n",
    "pos = arange(5)+.5    # the bar centers on the y axis\n",
    "\n",
    "figure(1)\n",
    "barh(pos,val, align='center')\n",
    "yticks(pos, ('Naive Bayes', 'SVM', 'Logistic_Regression', 'KNN', 'MLP'))\n",
    "xlabel('Accuracy')\n",
    "title('Accuracy Using SURF')\n",
    "grid(True)\n",
    "\n",
    "figure(2)\n",
    "barh(pos,val, xerr=rand(5), ecolor='r', align='center')\n",
    "yticks(pos, ('Naive Bayes', 'SVM', 'Logistic_Regression', 'KNN', 'MLP'))\n",
    "xlabel('Accuracy')\n",
    "\n",
    "show()"
