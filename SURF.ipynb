   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "N = 10\n",
    "ind = np.arange(N)  # the x locations for the groups\n",
    "width = 0.15       # the width of the bars\n",
    "fig, ax = plt.subplots()\n",
    "rects1 = ax.bar(ind, nb_acc, width, color='r')\n",
    "rects2 = ax.bar(ind + width, knn_acc, width, color='y')\n",
    "rects3 = ax.bar(ind + 2*width, mlp_acc, width, color='b')\n",
    "rects4 = ax.bar(ind + 3*width, svm_acc, width, color='orange')\n",
    "rects5 = ax.bar(ind + 4*width, lr_acc, width, color='g')\n",
    "# add some text for labels, title and axes ticks\n",
    "ax.set_ylabel('Accuracy')\n",
    "ax.set_xlabel('Number of Clusters')\n",
    "ax.set_title('Model accuracies')\n",
    "ax.set_xticks(ind + width / 2)\n",
    "ax.set_xticklabels(('50', '100', '150', '200', '250','300','350','400','450','500'))\n",
    "#ax.set_yticklabels(('0.10,0.30,0.50,0.70,1,1.10'))\n",
    "ax.legend(loc=3,hndles=(rects1[0], rects2[0],rects3[0],rects4[0],rects5[0]), label=('Naive Bayes', 'KNN','MLP','SVM','Logistic Reg.'))\n",
    "plt.show()"
   ]
  }
